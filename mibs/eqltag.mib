eqlUserDefinedTagLookupTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF EqlUserDefinedTagLookupEntry      
    MAX-ACCESS      not-accessible      
    STATUS          current      
    DESCRIPTION     "EqualLogic-Dynamic
                     This table allows for a faster lookup of the eqlTagType 
                     for a user defined tag name, by utilizing the
                     eqlUserDefinedTagTypeKeyName secondary key."  
    ::=  { eqltagObjects 7 }

eqlUserDefinedTagLookupEntry OBJECT-TYPE      
    SYNTAX          EqlUserDefinedTagLookupEntry      
    MAX-ACCESS      not-accessible      
    STATUS          current      
    DESCRIPTION     "An entry(row) which uses the eqlUserDefinedTagTypeKeyName
                     secondary key to lookup an eqlTagType."
    INDEX           { eqlUserDefinedTagTypeKeyName, eqlTagType }

    ::=   { eqlUserDefinedTagLookupTable 1 }

EqlUserDefinedTagLookupEntry ::=
    SEQUENCE {
        eqlUserDefinedTagLookupIndex Unsigned32
    }

eqlUserDefinedTagLookupIndex OBJECT-TYPE      
    SYNTAX          Unsigned32    
    MAX-ACCESS      read-only      
    STATUS          current      
    DESCRIPTION     "This field is the value of the eqlTagType of the 
                     corresponding entry matching the 
                     eqlUserDefinedTagTypeKeyName in the eqlUserDefinedTagTypeTable"  

    ::=  { eqlUserDefinedTagLookupEntry 1 }  
    
    
-------------------------------------------------------------------------------
--  Dynamic Tag Usage Count Table
--
-------------------------------------------------------------------------------
    
eqlTagUsageCountTable OBJECT-TYPE
    SYNTAX  SEQUENCE OF EqlTagUsageCountEntry      
    MAX-ACCESS      not-accessible      
    STATUS          current      
    DESCRIPTION     "EqualLogic-Dynamic
                     This table retrieves the usage count for a tag, based on
                     the admin access level of the user and the status of
                     the volume with the tag applied. A tag applied to a volume
                     that is not visible is not counted, such as a volume in the 
                     recovery bin, or a replicated volume that is not in the
                     primary or failover state. A Tag applied to a non-active 
                     sync-rep volume is not counted."  
    ::=  { eqltagObjects 8 }

eqlTagUsageCountEntry OBJECT-TYPE      
    SYNTAX          EqlTagUsageCountEntry      
    MAX-ACCESS      not-accessible      
    STATUS          current      
    DESCRIPTION     "An entry(row) which uses the eqlTagType and eqlTagIndex of
                     the tag to lookup the usage count."
    INDEX           { eqlTagType, eqlTagIndex }

    ::=   { eqlTagUsageCountTable 1 }

    EqlTagUsageCountEntry ::=
    SEQUENCE {
        eqlTagUsageCount Unsigned32
    }
    
    eqlTagUsageCount OBJECT-TYPE      
    SYNTAX          Unsigned32    
    MAX-ACCESS      read-only      
    STATUS          current      
    DESCRIPTION     "This field is the usage count for tag."  

    ::=  { eqlTagUsageCountEntry 1 }    

END

